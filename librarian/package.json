{
  "name": "library-attendant-interface",
  "version": "1.1.0",
  "description": "The web application for a library attendant to assist patrons with checking games in and out of the library. It also displays other relevant information to the attendants.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --config webpack.dev.js --open --hot --port 1234",
    "build:test": "webpack --config webpack.test.js -o dist/test/bundle.js -p",
    "build:prod": "webpack --config webpack.prod.js -o dist/prod/bundle.js -p",
    "build:mm": "webpack --config webpack.mm.js -o dist/mm/bundle.js -p",
    "build:all": "npm run build:test && npm run build:prod && npm run build:mm",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
